{
  "name": "phaser3-parcel-template",
  "version": "1.0.0",
  "description": "A typescript template project for Phaser 3 using Parceljs",
  "scripts": {
    "start-client": "parcel src/client/index.html -p 10001",
    "start-server": "ts-node-dev --project tsconfig.server.json --respawn --transpile-only src/server/index.ts",
    "build": "parcel build src/client/index.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx"
  },
  "author": "supertommy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ourcade/phaser3-parcel-template.git"
  },
  "homepage": "https://github.com/ourcade/phaser3-parcel-template",
  "devDependencies": {
    "@colyseus/uwebsockets-transport": "^0.14.24",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "copyfiles": "^2.4.1",
    "eslint": "^6.8.0",
    "minimist": "^1.2.5",
    "parcel": "^2.3.2",
    "rimraf": "^2.7.1",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^3.8.3",
    "uwebsockets-express": "^1.1.14"
  },
  "dependencies": {
    "@colyseus/arena": "^0.14.24",
    "@colyseus/command": "^0.2.1",
    "@colyseus/monitor": "^0.14.22",
    "colyseus": "^0.14.17",
    "colyseus.js": "^0.14.13",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "phaser": "^3.55.2",
    "serve-index": "^1.8.0",
    "superagent": "^3.8.1"
  },
  "parcelCleanPaths": [
    "dist"
  ],
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  }
}
